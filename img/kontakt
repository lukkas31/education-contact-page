<script type="text/javascript">require(['jquery','ui42-block','bootstrap'],function($,block){$(function(){$('.js-stores-section').each(function(){const section=$(this)
const filter=section.find('.js-stores-filter')
const sectionBody=section.find('.js-stores-section-body')
function initMapMarker(){sectionBody.find('.js-stores-map-marker').each(function(){const tooltipHtml=$(this).html()
if(tooltipHtml){$(this).popover({content:tooltipHtml,placement:'bottom',html:true,container:$(this),sanitize:false,trigger:'hover'}).on({'shown.bs.tooltip':function(){$(this).addClass('tooltip-active')},'hidden.bs.tooltip':function(){$(this).removeClass('tooltip-active')},})}})}initMapMarker()
filter.on('change',function(){block.block()
$.ajax({type:'get',url:$(this).val(),success:function(data){const response=$(data)
if(response.is('.js-stores')){sectionBody.html(response.get(0).outerHTML)
window.lazyLoadInstance.update()
initMapMarker()
block.unblock()}},error:function(data){console.log(data)
block.unblock()}})})})})})</script>
